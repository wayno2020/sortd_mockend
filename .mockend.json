{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "Education App Schema",
	"type": "object",
	"properties": {
		"students": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string"
					},
					"age": {
						"type": "integer"
					},
					"gender": {
						"type": "string"
					},
					"courses": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				},
				"required": ["name", "age", "gender", "courses"]
			}
		},
		"courses": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string"
					},
					"subject": {
						"type": "string"
					},
					"level": {
						"type": "string"
					},
					"students": {
						"type": "array",
						"items": {
							"type": "string"
						}
					}
				},
				"required": ["name", "subject", "level", "students"]
			}
		}
	},
	"required": ["students", "courses"]
}
